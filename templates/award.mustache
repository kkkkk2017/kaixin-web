<section id="award" class="scrollto clearfix wow fadeInUp" data-wow-delay="0.1s">
    <div class="row clearfix">

        <div class="col-3">

            <div class="section-heading">
                <h2 class="section-title">Awards, Grants & Academic Services</h2>
            </div>

        </div>

        <div class="col-2-3">
            {{#awards}}
            <div class="col-9-10 wow fadeInUp" data-wow-delay="0.3s">
                <div class="research-overlay">
                    <p>{{title}}</p></br>
                </div>
                </div>
            {{/awards}}

        <div class="col-9-10 wow fadeInUp" data-wow-delay="0.3s">
           <h4 class="link-title">Invited Reviewer & Sub-reviewer</h4>
            {{#services}}
                {{#title}}
                <div class="research-overlay">
                    <span>{{.}}</span><br>
                </div>
                {{/title}}
            {{/services}}
        </div>
        </div>
    </div>
</section>
